            </Label>
            <ComboBox fx:id="cbFilter" prefWidth="430.0" />
            <Separator prefWidth="500.0">
               <FlowPane.margin>
                  <Insets bottom="8.0" top="8.0" />
               </FlowPane.margin>
            </Separator>
         </children>
         <BorderPane.margin>
            <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
         </BorderPane.margin>
      </FlowPane>
