<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane maxHeight="600.0" maxWidth="512.0" prefHeight="600.0" prefWidth="512.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.screenflow.frankenstein.fxml.FilterSetupController">
   <bottom>
      <Button mnemonicParsing="false" onAction="#ok" text="OK" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="12.0" top="12.0" />
         </BorderPane.margin>
      </Button>
   </bottom>
   <top>
      <FlowPane BorderPane.alignment="CENTER">
         <children>
            <Label text="Filter:">
               <FlowPane.margin>
                  <Insets right="8.0" />
               </FlowPane.margin>
            </Label>
            <ComboBox fx:id="cbFilter" prefWidth="430.0" />
            <Separator prefWidth="500.0">
               <FlowPane.margin>
                  <Insets bottom="8.0" top="8.0" />
               </FlowPane.margin>
            </Separator>
         </children>
         <BorderPane.margin>
            <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
         </BorderPane.margin>
      </FlowPane>
   </top>
   <center>
      <GridPane BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
      </GridPane>
   </center>
</BorderPane>
