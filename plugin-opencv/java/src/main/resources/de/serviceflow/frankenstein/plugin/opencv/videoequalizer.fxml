<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <GridPane BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="10.0" prefWidth="300.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="%lBRIGHTNESS" />
            <Label text="%lSATURATION" GridPane.rowIndex="1" />
            <Label text="%lCONTRAST" GridPane.rowIndex="2" />
            <Slider fx:id="slBrightness" majorTickUnit="10.0" minorTickCount="1" showTickMarks="true" value="50.0" GridPane.columnIndex="1" />
            <Slider fx:id="slSaturation" majorTickUnit="10.0" minorTickCount="1" showTickMarks="true" value="50.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Slider fx:id="slContrast" majorTickUnit="10.0" minorTickCount="1" showTickMarks="true" value="50.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </top>
</BorderPane>
